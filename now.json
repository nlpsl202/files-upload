{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        {
            "src": "/uploads",
            "dest": "/uploads"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}